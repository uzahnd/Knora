# @prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
# @prefix skos: <http://www.w3.org/2004/02/skos/core#>.
# @prefix dct: <http://purl.org/dc/terms/>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix cidoc: <http://www.cidoc-crm.org/cidoc-crm/>.
@prefix frbroo: <http://iflastandards.info/ns/fr/frbr/frbroo/>.
@prefix event: <http://www.knora.org/ontology/event#>.
@prefix human: <http://www.knora.org/ontology/human#>.
@prefix work: <http://www.knora.org/ontology/work#>.
@prefix image: <http://www.knora.org/ontology/image#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.
@prefix salsah-gui: <http://www.knora.org/ontology/salsah-gui#>.
@prefix text: <http://www.knora.org/ontology/text#>.

<http://www.knora.org/ontology/text>
	a owl:Ontology.
#	dct:title "An ontology about text"@en;
#	dct:description """Formal description of text in broadest sense, general subclasses, related classes, and properties."""@en;
#	dct:creator "Hans Cools, software developer, NIE-INE, Basel University, Switzerland"@en;
#	dct:contributor:
#	skos:note """Representation of an otherwise literal data element as object ('objectivation') enabling relating it to metadata"""@en.

#
# CLASSES
#

text:Expression
	a owl:Class;
	rdfs:label "text expression"@en, "Text-Expression"@de;
	rdfs:comment """Text as a human luinguistic expression."""@en;
	rdfs:subClassOf work:Expression, cidoc:E33_Linguistic_Object, [
		a owl:Restriction; owl:onProperty text:hasAuthor; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAuthorValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasStructure; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasStructureValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasIncipit; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasExplicit; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasContent; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEdition; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEditionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDiplomaticTranscription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDiplomaticTranscriptionValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isOnCarrier; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isOnCarrierValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasPageIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasPageNumberIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasColumnIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasLineNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasVersionIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isInDialect; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:WrittenText
	a owl:Class;
	rdfs:label "written text"@en, "geschriebener Text"@de;
	rdfs:comment """Text written by any method, e.g. manually, with an analogue or digital typewriter, or on a computer."""@en;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty text:isOnCarrier; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isOnCarrierValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

text:HandwrittenText
	a owl:Class;
	rdfs:label "handwritten text"@en, "handgeschriebener Text"@de;
	rdfs:comment """Text written by hand."""@en;
	rdfs:subClassOf text:WrittenText, [
		a owl:Restriction; owl:onProperty text:isInManuscript; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isInManuscriptValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

text:TypewrittenText
	a owl:Class;
	rdfs:label "typewritten text"@en, "maschinengeschriebener Text"@de;
	rdfs:comment """Text written with a typewriter."""@en;
	rdfs:subClassOf text:WrittenText, [
		a owl:Restriction; owl:onProperty text:isInTypescript; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isInTypescriptValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

text:Author
	a owl:Class;
	rdfs:label "author"@en, "Autor"@de;
	rdfs:comment """Role of a person being responsible for a text."""@en;
#	skos:note """Often an author is also the creator of a text."""@en;
	rdfs:subClassOf human:Role.

text:Version
	a owl:Class;
	rdfs:label "text version"@en, "Textversion"@de;
	rdfs:comment """Version of a text."""@en;
	rdfs:subClassOf text:Expression.

text:Commentary
	a owl:Class;
	rdfs:label "commento"@la, "commentary"@en, "Erläuterung"@de;
	rdfs:comment """Text expression in the form of a series of comments as analysis, interpretation or explanation of something, especially another textual expression."""@en;
	rdfs:subClassOf text:Expression.

text:Revisor
	a owl:Class;
	rdfs:label "revisor"@en, "Revisor"@de;
	rdfs:comment """Role of a person checking on certain aspects of a text."""@en;
	rdfs:subClassOf human:Role. 

text:Derivative
	a owl:Class;
	rdfs:label "text derivative"@en, "abgeleitete Text"@de;
	rdfs:comment """Text expression derived from another text expression."""@en;
	rdfs:subClassOf text:Expression.

text:Edition
	a owl:Class;
	rdfs:label "text edition"@en, "Text Edition"@de; # Lesetext subClassOf
	rdfs:comment """Changed form of a text expression as the result of editing."""@en;
	rdfs:subClassOf text:Derivative.

text:Editor
	a owl:Class;
	rdfs:label "text editor"@en, "Text-Redakteur"@de;
	rdfs:comment """Role of a person changing a text."""@en;
	rdfs:subClassOf human:Role.

text:DiplomaticTranscription
	a owl:Class;
	rdfs:label "diplomatic transcription"@en, "diplomatische Umschrift"@de;
	rdfs:comment """Transcription of a text with diacritical signs indicating visual textual properties to describe the textual state."""@en;
	rdfs:subClassOf text:Derivative.

text:Carrier
 	a owl:Class;
 	rdfs:label "text carrier"@en,"Textträger"@de;
 	rdfs:comment """Carrier of a text expression."""@en;
	rdfs:subClassOf work:Manifestation, [
		a owl:Restriction; owl:onProperty text:hasCarrierDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Manuscript
	a owl:Class;
	rdfs:label "manuscript"@en, "Manuskript"@de;
	rdfs:comment """Carrier of a handwritten text expression."""@en;
	rdfs:subClassOf text:Carrier, frbroo:F4_Manifestation_Singleton.

text:NoteCarrier
	a owl:Class;
	rdfs:label "note carrier"@en,"Notizträger"@de;
	rdfs:comment """Carrier whereon notes are written, e.g. a first registration of thoughts."""@en;
	rdfs:subClassOf text:Carrier. # frbroo:F4_Manifestation_Singleton.

text:Notebook
	a owl:Class;
	rdfs:label "notebook"@en,"Notizbuch"@de;
	rdfs:comment """Book wherein notes are written."""@en;
	rdfs:subClassOf work:Book, text:NoteCarrier.

text:Typescript
	a owl:Class;
	rdfs:label "typescript"@en, "Typoskript"@de;
	rdfs:comment """Carrier of a typewritten text expression."""@en;
	rdfs:subClassOf text:Carrier. # frbroo:F4_Manifestation_Singleton.

text:Transcript
	a owl:Class;
	rdfs:label "transcript"@en, "Abschrift"@de;
	rdfs:comment """Carrier of a reproduced text by any method, e.g. handwriting, typewriting, machine copying, printing, digitizing."""@en;
	rdfs:subClassOf text:Carrier. # frbroo:F4_Manifestation_Singleton.

text:Diary
	a owl:Class;
	rdfs:label "diary"@en,"Tagebuch"@de;
	rdfs:comment """Text carrier with periodically, usually daily, described personal experiences and observations."""@en;
	rdfs:subClassOf text:Carrier.

text:DiaryEntry
	a owl:Class;
	rdfs:label "diary entry"@en, "Tagebucheintrag"@de;
	rdfs:comment """Periodical, usually daily description of a personal experience or observation written in a diary."""@en;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty text:isInDiary; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:isInDiaryValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEnteringDate; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Letter
	a owl:Class;
	rdfs:label "letter"@en, "Brief"@de;
	rdfs:comment """Written text intended to be sent from (a) person(s) or (an) organization(s) to another one, e.g. through a postal system."""@en;
	rdfs:subClassOf text:Carrier, frbroo:F4_Manifestation_Singleton, [
		a owl:Restriction; owl:onProperty text:hasSender; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSenderValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasReceiver; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasReceiverValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Postcard
	a owl:Class;
	rdfs:label "postcard"@en, "Postkarte"@de;
	rdfs:comment """Card with one side, usually with a printed structure, used for writing a message, an address and applying a postage stamp, to be sent through a postal system."""@en;
	rdfs:subClassOf human:Creation, frbroo:F4_Manifestation_Singleton, [
		a owl:Restriction; owl:onProperty text:hasSender; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSenderValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasReceiver; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasReceiverValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Convolute
	a owl:Class;
	rdfs:label "convolute"@en, "Konvolut"@de;
	rdfs:comment """Text carriers, e.g. of drafts, manuscripts, typoscripts or prints, gathered as one."""@en;
	rdfs:subClassOf human:Creation, cidoc:E78_Collection, [
		a owl:Restriction; owl:onProperty text:containsEarlierStagesOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:containsEarlierStagesOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:containsEarlierStagesOfPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:containsEarlierStagesOfPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:containsLaterStagesOf; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:containsLaterStagesOfValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteSizeDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteOriginDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteContentRepresentation; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:TextDevelopment
	a owl:Class;
	rdfs:label "text development"@en, "Text-Entwicklung"@de;
	rdfs:comment """Action wherein a person(s) creates text resulting in different manifestation, e.g. from notebook to manuscript to typescript to published print."""@en;
	rdfs:subClassOf human:Creating, [
		a owl:Restriction; owl:onProperty event:hasStage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty event:hasStageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger].

text:Structure
	a owl:Class;
	rdfs:label "text structure"@en, "Textstruktur"@de;
	rdfs:comment """."""@en;
	rdfs:subClassOf cidoc:E33_Linguistic_Object, knora-base:Resource.

text:Prose
	a owl:Class;
	rdfs:label "prose"@en, "Prosa"@de; # Prosanotat
	rdfs:comment """Grammatical text without metrical structure."""@en;
	rdfs:subClassOf text:Structure.

text:Verse
	a owl:Class;
	rdfs:label "verse"@en, "Verse"@de;
	rdfs:comment """Text with metrical structure."""@en;
	rdfs:subClassOf text:Structure.

text:FreeVerse
	a owl:Class;
	rdfs:label "free verse"@en, "reimlose Verse"@de;
	rdfs:comment """Verse without rhyming."""@en;
	rdfs:subClassOf text:Verse.

text:RhymingVerse
	a owl:Class;
	rdfs:label "rhyming verse"@en, "gereimte Verse"@de;
	rdfs:comment """Verse with rhyming."""@en;
	rdfs:subClassOf text:Verse.

# Prosaic Verse, Prosagedicht

text:RhythmicProse
	a owl:Class;
	rdfs:label "rhythmic Prose"@en, "rhythmische Prosa"@de;
	rdfs:comment """Prose ... ."""@en;
	rdfs:subClassOf text:Structure.

text:Poem # Poetry
	a owl:Class;
	rdfs:label "poem"@en, "Gedicht"@de;
	rdfs:comment """Text epression in verse."""@en, """Text-Epression in Versform."""@de;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty text:hasStrophe; owl:minCardinality "0"^^xsd:nonNegativeInteger]#, [
#		a owl:Restriction; owl:onProperty text:hasStructure; owl:someValuesFrom text:Verse]
	.

text:Poet
	a owl:Class;
	rdfs:label "poet"@en, "Dichter"@de;
	rdfs:comment """Author of a poem."""@en;
	rdfs:subClassOf text:Author.

#
# PROPERTIES
#

text:hasText
	a owl:ObjectProperty;
	rdfs:label "has text"@en, "hat Text"@de;
	rdfs:comment """Relating a resource to a text (as object) as a writable coherent set of signs representing semantics in a language."""@en;
	rdfs:subPropertyOf knora-base:hasValue;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasName
	a owl:ObjectProperty;
	rdfs:label "has name"@en, "hat Name"@de;
	rdfs:comment """Relating a resource to a name (as object) as a (non-unique) textual identifier of the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasPreferredName
	a owl:ObjectProperty;
	rdfs:label "has preferred name"@en, "hat bevorzugter Name"@de;
	rdfs:comment """Relating a resource to a preferred name (as object)."""@en;
	rdfs:subPropertyOf text:hasName;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasAlias
	a owl:ObjectProperty;
	rdfs:label "has alias"@en, "hat Alias"@de;
	rdfs:comment """Relating a resource to an alias (as object) as an alternative name of the resource."""@en;
	rdfs:subPropertyOf text:hasName;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasIdentifier
	a owl:ObjectProperty;
	rdfs:label "has identifier"@en, "hat Kennzeichen"@de;
	rdfs:comment """Relating a resource to an identifier (as object) as a unique textual reference to the resource within a given context, and conforming to a formal identification system."""@en;
	rdfs:subPropertyOf text:hasText; # cidoc:E42_Identifier, cf. dcterms:identifier
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasTitle
	a owl:ObjectProperty;
	rdfs:label "has title"@en, "hat Titel"@de;
	rdfs:comment """Relating a resource to a title (as object) as a textual identifier of the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasComment
	a owl:ObjectProperty;
	rdfs:label "has comment"@en, "hat Kommentar"@de;
	rdfs:comment """Relating a resource to a comment (as object) as a textual statement about the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDescription
	a owl:ObjectProperty;
	rdfs:label "has description"@en, "hat Beschreibung"@de;
	rdfs:comment """Relating a resource to a description (as object) as a textual statement about features of the resource."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDefinition
	a owl:ObjectProperty;
	rdfs:label "has definition"@en, "hat Definition"@de;
	rdfs:comment """Relating a resource to a definition (as object) as a textual statement about the essential features of the resource."""@en;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDetailDescription
	a owl:ObjectProperty;
	rdfs:label "has detail description"@en, "hat Detail-Beschreibung"@de;
	rdfs:comment """Relating a resource to a description (as object) of its detail(s). E.g. in the case of a text a detail can be a passage variant or an emendation"""@en;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasSpecialDescription
	a owl:ObjectProperty;
	rdfs:label "has special description"@en, "hat Besonderes-Beschreibung"@de;
	rdfs:comment """Relating a resource to a description (as object) of (a) certain feature(s)."""@en;
	rdfs:subPropertyOf text:hasDescription;
	knora-base:subjectClassConstraint knora-base:Resource;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasStructure
	a owl:ObjectProperty;
	rdfs:label "has structure"@en, "hat Struktur"@de;
	rdfs:comment """Relating a text expression to its structure."""@en;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint text:Structure.

text:hasStructureValue
	a owl:ObjectProperty;
	rdfs:label "has structure - statement"@en, "hat Struktur - Äusserung"@de;
	rdfs:comment """Relating a text expression to a reification statement of the relation between the text expression and its structure."""@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint knora-base:LinkValue.

text:hasIncipit
	a owl:ObjectProperty;
	rdfs:label "has incipit"@en, "hat Incipit"@de;
	rdfs:comment """Relating an untitled text expression to a beginning part thereof (as object) to identify the text."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasExplicit
	a owl:ObjectProperty;
	rdfs:label "has explicit"@en, "hat Explicit"@de;
	rdfs:comment """Relating an untitled text expression to an end part thereof (as object) to identify the text."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasContent
	a owl:ObjectProperty;
	rdfs:label "has content"@en, "hat Inhalt"@de;
	rdfs:comment """Relating a text expression to its content (as object)."""@en;
	rdfs:subPropertyOf text:hasText;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint knora-base:TextValue;
	salsah-gui:guiElement salsah-gui:Richtext.

text:hasEdition
	a owl:ObjectProperty;
	rdfs:label "has edition"@en, "hat Edition"@de;
	rdfs:comment "Relating a text to an edition thereof."@en;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint text:Edition.

text:hasEditionValue
	a owl:ObjectProperty;
	rdfs:label "has edition - statement"@en, "hat Edition - Äusserung"@de;
	rdfs:comment "Relating a text to a reification statement of the relation between the text and an edition thereof."@en;
	rdfs:subPropertyOf knora-base:hasLinkToValue;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint knora-base:LinkValue.

#:hasEditedContent
#	a owl:ObjectProperty;
#	rdfs:label "has edited content"@en;
#	rdfs:comment """Relating an edition to its content (as object)."""@en;
#	rdfs:subPropertyOf text:hasContent;
#	knora-base:subjectClassConstraint text:Edition;
#	knora-base:objectClassConstraint knora-base:TextValue;
#	salsah-gui:guiElement salsah-gui:Richtext.

text:hasDiplomaticTranscription
	a owl:ObjectProperty;
	rdfs:label "has diplomatic transcription"@en, "hat diplomatische Umschrift"@de;
	rdfs:comment "Relating a text to a diplomatic transcription thereof."@en;
	rdfs:subPropertyOf knora-base:hasLinkTo;
	knora-base:subjectClassConstraint text:Expression;
	knora-base:objectClassConstraint text:DiplomaticTranscription.

text:hasDiplomaticTranscriptionValue
	a owl:ObjectProperty;
	rdfs:label "has diplomatic transcription - statement"@en, "hat diplomatische Umschrift - Äusserung"@de;
	rdfs:comment "Relating a text to a reification statement of the relation between the text and a diplomatic transcription thereof."@en;

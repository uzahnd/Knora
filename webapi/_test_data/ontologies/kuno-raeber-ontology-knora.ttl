#		a owl:Restriction; owl:onProperty text:containsEarlierStagesOfPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at text:Convolute
		a owl:Restriction; owl:onProperty text:hasConvoluteSizeDescription; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteContentRepresentation; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:PoemManuscript
	a owl:Class;
	rdfs:label "poem manuscript"@en, "Gedicht-Manuskript"@de;

	a owl:Class;
	rdfs:label "diary"@en,"Tagebuch"@de;
	rdfs:comment """Diary with entries authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:Diary, work:Book, work:ArchiveElement, [
		a owl:Restriction; owl:onProperty work:hasArchiveSignature; owl:minCardinality "1"^^xsd:nonNegativeInteger].

# Note: a Raeber archive signature could look like "C-2-a/05"^^:archive instead of a plain literal (or string).
# :archive a rdfs:Datatype.

:DiaryConvolute
	a owl:Class;
	rdfs:label "diary convolute"@en,"Tagebuch-Konvolut"@de;
	rdfs:comment """Convolute of diaries with entries authored by Kuno Raeber."""@en;
	rdfs:subClassOf :Convolute, work:ArchiveElement, [
		a owl:Restriction; owl:onProperty human:hasCreatingPeriod; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty human:hasCreatingPeriodValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteSizeDescription; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteContentRepresentation; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:hasArchiveSignature; owl:minCardinality "1"^^xsd:nonNegativeInteger].

:NachlassPublication
	a owl:Class;
	rdfs:label "nachlass publication"@en, "Nachlass-Publication"@de;
	rdfs:comment """Publication of a Nachlass of Kuno Raeber."""@en;
	rdfs:subClassOf work:NachlassPublication, work:Volume, text:Carrier, work:PrintedPublication, [
		a owl:Restriction; owl:onProperty work:hasPublicationTitle; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:hasPublicationDate; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:hasPublisherDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:hasVolumeNumber; owl:cardinality "1"^^xsd:nonNegativeInteger].


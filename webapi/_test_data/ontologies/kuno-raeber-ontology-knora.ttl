@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
# @prefix dct: <http://purl.org/dc/terms/>.
@prefix human: <http://www.knora.org/ontology/human#>.
@prefix work: <http://www.knora.org/ontology/work#>.
@prefix text: <http://www.knora.org/ontology/text#>.
@prefix image: <http://www.knora.org/ontology/image#>.
@prefix knora-base: <http://www.knora.org/ontology/knora-base#>.
@prefix salsah-gui: <http://www.knora.org/ontology/salsah-gui#>.
@prefix kuno-raeber: <http://www.knora.org/ontology/kuno-raeber#>.

<http://www.knora.org/ontology/kuno-raeber>
	a owl:Ontology.
#	dct:title "An ontology about the lyric of Kuno Raeber"@en;
#	dct:description """Formal description of elements in the online publication of the lyric work of the Swiss poet Kuno Raeber."""@en;
#	dct:creator "Hans Cools, software developer, NIE-INE, Basel University, Switzerland"@en.

#
# CLASSES
#

kuno-raeber:Work
	a owl:Class;
	rdfs:label "work"@en, "Werk"@de;
	rdfs:comment """Work by Kuno Raeber."""@en, """Werk von Kuno Raeber."""@de;
	rdfs:subClassOf work:IndividualWork, [
		a owl:Restriction; owl:onProperty text:hasTitle; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isExpressedIn; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isExpressedInValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:Text
	a owl:Class;
	rdfs:label "text expression"@en, "Text-Expression"@de;
	rdfs:comment """Text authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty text:hasAuthor; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAuthorValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasTitle; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAlias; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty knora-base:seqnum; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasEdition; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasEditionValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:Poem
	a owl:Class;
	rdfs:label "poem"@en, "Gedicht"@de;
	rdfs:comment """Poem authored by Kuno Raeber."""@en, """Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf text:Poem, kuno-raeber:Text, [
		a owl:Restriction; owl:onProperty human:hasCreationDate; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDetailDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSpecialDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger]#, [
#		a owl:Restriction; owl:onProperty work:hasPublishingState; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at work:Expression
#		a owl:Restriction; owl:onProperty work:hasPublishingStateValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at work:Expression
#		a owl:Restriction; owl:onProperty work:hasLastPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at work:Expression
#		a owl:Restriction; owl:onProperty work:hasLastPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger] # declared at work:Expression
		.

kuno-raeber:DiaryEntry
	a owl:Class;
	rdfs:label "diary entry"@en, "Tagebucheintrag"@de;
	rdfs:comment """Diary entry authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:DiaryEntry, kuno-raeber:Text, [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiary; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiaryValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasEnteringDate; owl:cardinality "1"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty work:isOnPage; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty work:isOnPageValue; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiaryConvolute; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInDiaryConvoluteValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:hasNachlassPublicationDescription; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:Edition
	a owl:Class;
	rdfs:label "edited text"@en, "edierte Text"@de;
	rdfs:comment """Edition of a text authored by Kuno Raeber."""@en, """Edition eines von Kuno Raeber verfassten Texts."""@de;
	rdfs:subClassOf text:Edition, [
		a owl:Restriction; owl:onProperty text:hasContent; owl:cardinality "1"^^xsd:nonNegativeInteger]#, [
#		a owl:Restriction; owl:onProperty text:hasPageIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
#		a owl:Restriction; owl:onProperty text:hasLineNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger]
		.

kuno-raeber:DiplomaticTranscription
	a owl:Class;
	rdfs:label "diplomatic transcription"@en, "diplomatische Umschrift"@de;
	rdfs:comment """Diplomatic transcription of a text authored by Kuno Raeber."""@en, """Diplomatische Umschrift eines von Kuno Raeber verfassten Texts."""@de;
	rdfs:subClassOf text:DiplomaticTranscription, [
		a owl:Restriction; owl:onProperty text:hasContent; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasLineNumber; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasVersionIndicator; owl:minCardinality "0"^^xsd:nonNegativeInteger].

kuno-raeber:HandwrittenPoem
	a owl:Class;
	rdfs:label "handwritten poem"@en, "handgeschriebenes Gedicht"@de;
	rdfs:comment """Handwritten poem authored by Kuno Raeber."""@en, """Handgeschriebenes Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:Poem, text:HandwrittenText, [
#		a owl:Restriction; owl:onProperty text:isInManuscript; owl:cardinality "1"^^xsd:nonNegativeInteger], [ # declared at text:HandwrittenText
#		a owl:Restriction; owl:onProperty text:isInManuscriptValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [ # declared at text:HandwrittenText
		a owl:Restriction; owl:onProperty kuno-raeber:hasDiplomaticTranscription; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:hasDiplomaticTranscriptionValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isOnPage; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isOnPageValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:PoemNote
	a owl:Class;
	rdfs:label "poem note"@en, "Gedichtnotiz"@de;
	rdfs:comment """Poem note authored by Kuno Raeber."""@en, """Gedichtnotiz von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:HandwrittenPoem, work:Draft, [
		a owl:Restriction; owl:onProperty kuno-raeber:isInNotebook; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty kuno-raeber:isInNotebookValue; owl:cardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:TypewrittenPoem
	a owl:Class;
	rdfs:label "typewritten poem"@en, "maschinengeschriebenes Gedicht"@de;
	rdfs:comment """Typewritten poem authored by Kuno Raeber."""@en, """Maschinengeschriebenes Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf kuno-raeber:Poem, text:TypewrittenText, [
		a owl:Restriction; owl:onProperty work:isOnPage; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isOnPageValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:PublicationPoem
	a owl:Class;
	rdfs:label "publication poem"@en, "Publikation-Gedicht"@de;
	rdfs:comment """Poem authored by Kuno Raeber as publication."""@en, """Gedicht von Kuno Raeber als Publikation."""@de;
	rdfs:subClassOf kuno-raeber:Poem, work:PublicationExpression, [
		a owl:Restriction; owl:onProperty work:isPublishedIn; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isPublishedInValue; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isOnPage; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isOnPageValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:hasNachlassPublicationDescription; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:Page # to be removed when knora-base:hasRepresentation will be implemented in Knora2
	a owl:Class;
	rdfs:label "page"@en, "Seite"@de;
	rdfs:comment """Page with a handwritten poem by Kuno Raeber."""@en, """Seite mit einem handgeschriebenen Gedicht Kuno Raebers."""@de;
	rdfs:subClassOf work:Page, knora-base:StillImageRepresentation, [
		a owl:Restriction; owl:onProperty work:hasPageNumber; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty knora-base:hasStillImageFileValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

kuno-raeber:Convolute
	a owl:Class;
	rdfs:label "convolute"@en, "Konvolut"@de;
	rdfs:comment """Text convolute of which the elements are authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:Convolute, [
		a owl:Restriction; owl:onProperty text:hasTitle; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAlias; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasConvoluteDescription; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasComment; owl:minCardinality "0"^^xsd:nonNegativeInteger].


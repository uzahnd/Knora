<http://www.knora.org/ontology/kuno-raeber>
	a owl:Ontology.
#	dct:title "An ontology about the lyric of Kuno Raeber"@en;
#	dct:description """Formal description of elements in the online publication of the lyric work of the Swiss poet Kuno Raeber."""@en;
#	dct:creator "Hans Cools, software developer, NIE-INE, Basel University, Switzerland"@en.

#
# CLASSES
#

:Work
	a owl:Class;
	rdfs:label "work"@en, "Werk"@de;
	rdfs:comment """Work by Kuno Raeber."""@en, """Werk von Kuno Raeber."""@de;
	rdfs:subClassOf work:IndividualWork, [
		a owl:Restriction; owl:onProperty text:hasTitle; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isExpressedIn; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty work:isExpressedInValue; owl:minCardinality "1"^^xsd:nonNegativeInteger].

:Text
	a owl:Class;
	rdfs:label "text expression"@en, "Text-Expression"@de;
	rdfs:comment """Text authored by Kuno Raeber."""@en;
	rdfs:subClassOf text:Expression, [
		a owl:Restriction; owl:onProperty text:hasAuthor; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAuthorValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasTitle; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasAlias; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty knora-base:seqnum; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty :hasEdition; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty :hasEditionValue; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty human:hasModificationDateTime; owl:cardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty :isPublishedOnline; owl:cardinality "1"^^xsd:nonNegativeInteger].

:Poem
	a owl:Class;
	rdfs:label "poem"@en, "Gedicht"@de;
	rdfs:comment """Poem authored by Kuno Raeber."""@en, """Gedicht von Kuno Raeber."""@de;
	rdfs:subClassOf text:Poem, :Text, [
		a owl:Restriction; owl:onProperty human:hasCreationDate; owl:minCardinality "1"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasDetailDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty text:hasSpecialDescription; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty :hasSameEditionAs; owl:minCardinality "0"^^xsd:nonNegativeInteger], [
		a owl:Restriction; owl:onProperty :hasSameEditionAsValue; owl:minCardinality "0"^^xsd:nonNegativeInteger]#, [
#		a owl:Restriction; owl:onProperty work:hasPublishingState; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at work:Expression
#		a owl:Restriction; owl:onProperty work:hasPublishingStateValue; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at work:Expression
#		a owl:Restriction; owl:onProperty work:hasLastPublication; owl:minCardinality "0"^^xsd:nonNegativeInteger], [ # declared at work:Expression
#		a owl:Restriction; owl:onProperty work:hasLastPublicationValue; owl:minCardinality "0"^^xsd:nonNegativeInteger] # declared at work:Expression
		.


